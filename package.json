{
  "name": "drug-wars-empire",
  "version": "2.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--no-experimental-strip-types expo start",
    "android": "cross-env NODE_OPTIONS=--no-experimental-strip-types expo start --android",
    "ios": "cross-env NODE_OPTIONS=--no-experimental-strip-types expo start --ios",
    "web": "cross-env NODE_OPTIONS=--no-experimental-strip-types expo start --web",
    "export": "cross-env NODE_OPTIONS=--no-experimental-strip-types expo export",
    "build": "expo export --platform web",
    "typecheck": "tsc --noEmit",
    "simulate": "npx tsx scripts/simulate.ts"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@react-native-async-storage/async-storage": "~2.1.0",
    "@supabase/supabase-js": "^2.45.0",
    "expo": "~52.0.0",
    "expo-av": "~15.0.0",
    "expo-constants": "~17.0.0",
    "expo-font": "~13.0.0",
    "expo-haptics": "~14.0.0",
    "expo-router": "~4.0.0",
    "expo-splash-screen": "~0.29.0",
    "expo-status-bar": "~2.0.0",
    "react": "^18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-reanimated": "~3.16.0",
    "react-native-safe-area-context": "~4.14.0",
    "react-native-screens": "~4.4.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.13",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~18.3.0",
    "cross-env": "^10.1.0",
    "typescript": "~5.3.0"
  }
}
